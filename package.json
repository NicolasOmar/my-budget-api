{
  "name": "my-budget-api",
  "version": "0.1.0",
  "description": "RESTful api based on NodeJs to give middle/back-end support for upcomming Angular and React web applications",
  "main": "node ./src/index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "local": "env-cmd -f ./config/local.env nodemon ./src/index.js",
    "test": "env-cmd -f ./config/test.env jest --watch --runInBand"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "author": "Nicolás Omar González Passerino",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "hbs": "^4.1.0",
    "jest": "^25.2.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.4",
    "multer": "^1.4.2",
    "supertest": "^4.0.2",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "chalk": "^3.0.0",
    "env-cmd": "^10.1.0",
    "nodemon": "^2.0.2"
  },
  "engines": {
    "node": "12.16.1",
    "npm": "6.13.7"
  }
}
